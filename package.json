{
  "name": "ft",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run -s watch",
    "bstart": "npm run build && NODE_ENV=production node build/server.js",
    "test": "wdio",
    "test-full": "npm run build && NODE_ENV=production npm test",
    "start-web": "node node_modules/nodemon/bin/nodemon --watch build/server.js ./build/server.js",
    "start-build": "node node_modules/concurrently/src/main --raw --kill-others \"node build/server.js\"",
    "deploy": "git push deis master",
    "gmp": "npm run -s git-merge-production",
    "ctest": "sh -e ./bin/e2e.sh",
    "postinstall": "grunt build",
    "git-merge-production": "git checkout production && git pull origin production && git merge master && git push && git checkout master || (git checkout master && echo '>' && echo '> !!!!! FAILED !!!!! DO THE MERGE MANUALLY !!!!!!!!!!!' && echo '>' && exit 1)",
    "build": "node_modules/.bin/concurrent \"npm run build-server\" \"npm run build-client\"",
    "build-server": "WP_BACKEND=1 node node_modules/webpack/bin/webpack --colors --display-error-details --config node_modules/derby-webpack/index.js",
    "build-client": "WP_FRONTEND=1 node node_modules/webpack/bin/webpack --colors --display-error-details --config node_modules/derby-webpack/index.js",
    "watch-server": "WP_BACKEND=1 WP_WATCH=1 node node_modules/webpack/bin/webpack --watch --verbose --colors --display-error-details --config node_modules/derby-webpack/index.js",
    "watch-server-start": "node node_modules/just-wait -t 1000 --pattern \"build/server.js\" && npm run -s start-web",
    "watch-client": "WP_FRONTEND=1 WP_WATCH=1 node node_modules/webpack-dev-server/bin/webpack-dev-server --config node_modules/derby-webpack/index.js",
    "watch": "node_modules/.bin/concurrent --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "start-prod-locally": "STAGE=production npm run build && STAGE=production node ./build/server.js",
    "precommit": "npm run lint",
    "lint": "./node_modules/.bin/coffeelint -f coffeelint.json ./app ./server ./model"
  },
